{
  "family": "auxite-watcher",
  "taskRoleArn": "arn:aws:iam::809278147371:role/auxite-watcherTaskRole",
  "executionRoleArn": "arn:aws:iam::809278147371:role/auxite-ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "auxite-watcher",
      "image": "809278147371.dkr.ecr.eu-central-1.amazonaws.com/auxite-watcher",
      "cpu": 0,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp",
          "name": "http"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "NODE_ENV",
          "value": "production"
        },
        {
          "name": "WEBHOOK_SECRET",
          "value": "auxite-shared-secret"
        },
        {
          "name": "DEBUG_WS",
          "value": "true"
        },
        {
          "name": "ALLOWED_ORIGINS",
          "value": "https://wallet.auxite.io,https://api.auxite.io"
        },
        {
          "name": "ALLOWED_HOSTS",
          "value": "api.auxite.io"
        },
        {
          "name": "WS_ALLOW_ALL",
          "value": "0"
        },
        {
          "name": "PORT",
          "value": "8080"
        }
      ],
      "mountPoints": [],
      "volumesFrom": [],
      "linuxParameters": {
        "initProcessEnabled": true
      },
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/auxite-watcher",
          "awslogs-region": "eu-central-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "healthCheck": {
        "command": [
          "CMD-SHELL",
          "curl -f http://localhost:8080/healthz || exit 1"
        ],
        "interval": 30,
        "timeout": 5,
        "retries": 3,
        "startPeriod": 15
      },
      "systemControls": []
    }
  ],
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}
