{
  "family": "auxite2-api",
  "taskRoleArn": "arn:aws:iam::809278147371:role/auxite2-task-role",
  "executionRoleArn": "arn:aws:iam::809278147371:role/auxite2-task-exec",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "api",
      "image": "809278147371.dkr.ecr.eu-central-1.amazonaws.com/auxite2/api:v4",
      "cpu": 0,
      "portMappings": [
        {
          "containerPort": 3001,
          "hostPort": 3001,
          "protocol": "tcp",
          "name": "api-3001-tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "PORT",
          "value": "3001"
        }
      ],
      "mountPoints": [],
      "volumesFrom": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/auxite2/api",
          "awslogs-region": "eu-central-1",
          "awslogs-stream-prefix": "api"
        },
        "secretOptions": []
      },
      "healthCheck": {
        "command": [
          "CMD-SHELL",
          "curl -f http://localhost:80/ || exit 1"
        ],
        "interval": 15,
        "timeout": 5,
        "retries": 3,
        "startPeriod": 10
      },
      "systemControls": []
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "512",
  "memory": "1024"
}
